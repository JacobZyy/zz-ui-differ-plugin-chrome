{
  "name": "@ui-differ/core",
  "version": "1.0.0",
  "description": "UI Differ Core Algorithm Library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --outDir dist --format esm,cjs --dts",
    "dev": "tsup src/index.ts --outDir dist --format esm,cjs --dts --watch",
    "clean": "rm -rf dist",
    "clean:modules": "rm -rf ./node_modules"
  },
  "devDependencies": {
    "tsup": "^8.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
